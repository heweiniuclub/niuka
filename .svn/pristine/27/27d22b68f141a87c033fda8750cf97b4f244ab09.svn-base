package com.niuka.store.model;

import com.alibaba.fastjson.annotation.JSONField;
import com.niuka.common.model.PersistentObject;

import java.math.BigDecimal;
import java.util.Date;

/**
  
*/
public class CustomerStoreStock extends PersistentObject {

	// 门店编号
	private String storeNo;
	// 创建时间
	@JSONField(format = "yyyy-MM-dd HH:mm:ss")
	private Date createtime;
	// 最后一次更新时间
	@JSONField(format = "yyyy-MM-dd HH:mm:ss")
	private Date modifiedTime;
	// 产品库ID
	private Integer commodityId;
	// 客户ID
	private Integer customerId;
	// 门店ID
	private Integer customerStoreId;
	// 客户库存ID
	private Integer customerStock;
	// 库存预警值
	private Integer stockWarningValue;
	// 产品条形码
	private String barCode;
	// 产品单位
	private String unit;
	// 商品库存
	private Integer storeActualStock;
	// 总金额
	private BigDecimal totalAmount;
	// 门店历史库存
	private Integer storeHistoryStock;
	// 门店异常库存
	private Integer storeExceptionStock;



	public void setStoreNo(String storeNo){
		this.storeNo = storeNo;
	}

	public String getStoreNo(){
		return this.storeNo;
	}

	public void setCreatetime(Date createtime){
		this.createtime = createtime;
	}

	public Date getCreatetime(){
		return this.createtime;
	}

	public void setModifiedTime(Date modifiedTime){
		this.modifiedTime = modifiedTime;
	}

	public Date getModifiedTime(){
		return this.modifiedTime;
	}

	public void setCommodityId(Integer commodityId){
		this.commodityId = commodityId;
	}

	public Integer getCommodityId(){
		return this.commodityId;
	}

	public void setCustomerId(Integer customerId){
		this.customerId = customerId;
	}

	public Integer getCustomerId(){
		return this.customerId;
	}

	public void setCustomerStoreId(Integer customerStoreId){
		this.customerStoreId = customerStoreId;
	}

	public Integer getCustomerStoreId(){
		return this.customerStoreId;
	}

	public void setCustomerStock(Integer customerStock){
		this.customerStock = customerStock;
	}

	public Integer getCustomerStock(){
		return this.customerStock;
	}

	public void setStockWarningValue(Integer stockWarningValue){
		this.stockWarningValue = stockWarningValue;
	}

	public Integer getStockWarningValue(){
		return this.stockWarningValue;
	}

	public void setBarCode(String barCode){
		this.barCode = barCode;
	}

	public String getBarCode(){
		return this.barCode;
	}

	public void setUnit(String unit){
		this.unit = unit;
	}

	public String getUnit(){
		return this.unit;
	}

	public void setStoreActualStock(Integer storeActualStock){
		this.storeActualStock = storeActualStock;
	}

	public Integer getStoreActualStock(){
		return this.storeActualStock;
	}

	public void setTotalAmount(BigDecimal totalAmount){
		this.totalAmount = totalAmount;
	}

	public BigDecimal getTotalAmount(){
		return this.totalAmount;
	}

	public void setStoreHistoryStock(Integer storeHistoryStock){
		this.storeHistoryStock = storeHistoryStock;
	}

	public Integer getStoreHistoryStock(){
		return this.storeHistoryStock;
	}

	public void setStoreExceptionStock(Integer storeExceptionStock){
		this.storeExceptionStock = storeExceptionStock;
	}

	public Integer getStoreExceptionStock(){
		return this.storeExceptionStock;
	}

}