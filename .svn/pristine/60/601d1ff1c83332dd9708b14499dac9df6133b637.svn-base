package com.niuka.store.model;

import com.alibaba.fastjson.annotation.JSONField;
import com.niuka.common.model.PersistentObject;

import java.math.BigDecimal;
import java.util.Date;

/**
  
*/
public class CustomerStoreCommodityBatch extends PersistentObject {

	// 创建时间
	@JSONField(format = "yyyy-MM-dd HH:mm:ss")
	private Date createtime;
	// 最后一次更新时间
	@JSONField(format = "yyyy-MM-dd HH:mm:ss")
	private Date modifiedTime;
	// 客户产品批次ID
	private Integer customerCommodityBatchId;
	// 客户门店库存ID
	private Integer customerStoreStockId;
	// 生产日期
	private Date dateOfManufacture;
	// 保质期预警值
	private Byte qualityGuaranteePeriodWarningValue;
	// 批次数量
	private Integer storeBatchNum;
	// 产品单位
	private String unit;
	// 总金额
	private BigDecimal totalAmount;
	// 产品进货价格
	private BigDecimal commodityPrice;
	// 门店批次剩余数量
	private Integer storeSurplusBatchNum;
	// 门店批次异常数量
	private Integer storeExceptionBatchNum;


	public void setCreatetime(Date createtime){
		this.createtime = createtime;
	}

	public Date getCreatetime(){
		return this.createtime;
	}

	public void setModifiedTime(Date modifiedTime){
		this.modifiedTime = modifiedTime;
	}

	public Date getModifiedTime(){
		return this.modifiedTime;
	}

	public void setCustomerCommodityBatchId(Integer customerCommodityBatchId){
		this.customerCommodityBatchId = customerCommodityBatchId;
	}

	public Integer getCustomerCommodityBatchId(){
		return this.customerCommodityBatchId;
	}

	public void setCustomerStoreStockId(Integer customerStoreStockId){
		this.customerStoreStockId = customerStoreStockId;
	}

	public Integer getCustomerStoreStockId(){
		return this.customerStoreStockId;
	}

	public void setDateOfManufacture(Date dateOfManufacture){
		this.dateOfManufacture = dateOfManufacture;
	}

	public Date getDateOfManufacture(){
		return this.dateOfManufacture;
	}

	public void setQualityGuaranteePeriodWarningValue(Byte qualityGuaranteePeriodWarningValue){
		this.qualityGuaranteePeriodWarningValue = qualityGuaranteePeriodWarningValue;
	}

	public Byte getQualityGuaranteePeriodWarningValue(){
		return this.qualityGuaranteePeriodWarningValue;
	}

	public void setStoreBatchNum(Integer storeBatchNum){
		this.storeBatchNum = storeBatchNum;
	}

	public Integer getStoreBatchNum(){
		return this.storeBatchNum;
	}

	public void setUnit(String unit){
		this.unit = unit;
	}

	public String getUnit(){
		return this.unit;
	}

	public void setTotalAmount(BigDecimal totalAmount){
		this.totalAmount = totalAmount;
	}

	public BigDecimal getTotalAmount(){
		return this.totalAmount;
	}

	public void setCommodityPrice(BigDecimal commodityPrice){
		this.commodityPrice = commodityPrice;
	}

	public BigDecimal getCommodityPrice(){
		return this.commodityPrice;
	}

	public void setStoreSurplusBatchNum(Integer storeSurplusBatchNum){
		this.storeSurplusBatchNum = storeSurplusBatchNum;
	}

	public Integer getStoreSurplusBatchNum(){
		return this.storeSurplusBatchNum;
	}

	public void setStoreExceptionBatchNum(Integer storeExceptionBatchNum){
		this.storeExceptionBatchNum = storeExceptionBatchNum;
	}

	public Integer getStoreExceptionBatchNum(){
		return this.storeExceptionBatchNum;
	}

}