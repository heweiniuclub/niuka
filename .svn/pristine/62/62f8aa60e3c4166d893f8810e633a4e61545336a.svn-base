package com.niuka.api.server.controller.param.input.login;

/**
 * Created by admin on 2017/9/11.
 */
public class LoginVO {

	/**
	 * 登录用户名
	 */
    private String name;
    
    /**
     * 登录密码
     */
    private String password;
    
    /**
     * 登录类型
     * @return
     */
    private Integer loginType;//登录类型    0第三方登录 1普通登录，2短信验证码快捷登录
    
    /**
     * 快捷登录类型
     * @return
     */
    private Integer oauthLoginType;//0 QQ登录 1微信登录
    
    /**
     * 验证码
     * @return
     */
    private String identifyingCode;
    
    /**
     * 权限登录名称
     * @return
     */
    private String oauthName;
    
    /**
     * 权限登录唯一ID
     * @return
     */
    private String oauthId;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getPassword() {
        return password;
    }

    public void setPassword(String password) {
        this.password = password;
    }

	public Integer getLoginType() {
		return loginType;
	}

	public void setLoginType(Integer loginType) {
		this.loginType = loginType;
	}

	public Integer getOauthLoginType() {
		return oauthLoginType;
	}

	public void setOauthLoginType(Integer oauthLoginType) {
		this.oauthLoginType = oauthLoginType;
	}

	public String getIdentifyingCode() {
		return identifyingCode;
	}

	public void setIdentifyingCode(String identifyingCode) {
		this.identifyingCode = identifyingCode;
	}

	public String getOauthName() {
		return oauthName;
	}

	public void setOauthName(String oauthName) {
		this.oauthName = oauthName;
	}

	public String getOauthId() {
		return oauthId;
	}

	public void setOauthId(String oauthId) {
		this.oauthId = oauthId;
	}
    
}
