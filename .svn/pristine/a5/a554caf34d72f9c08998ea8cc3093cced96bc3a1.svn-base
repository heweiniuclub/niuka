.dialog-mask {
	position: fixed;
	z-index: 10;
	width: 100%;
	height: 100%;
	background: black;
	opacity: 0;
	top: 0px;
	left: 0px;
	-webkit-transition: all 0.5s;
	-moz-transition: all 0.5s;
	-ms-transition: all 0.5s;
	transition: all 0.5s;
}

.dialog-box {
	position: fixed;
	z-index: 11;
	background: white;
	width: 400px;
	top: 300px;
	left: calc((100% - 400px)/2);
	overflow: auto;
}

.dialog-title {
	text-indent: 10px;
	font-size: 15px;
	height: 45px;
	line-height: 45px;
	border-bottom: 1px solid #e1e1e1;
}

.dialog-content {
	padding-left: 10px;
	padding-right: 10px;
	padding-top: 25px;
	padding-bottom: 25px;
	text-align: center;
	font-size: 16px;
}
.dialog-ok{
	float: right;
	margin-right: 12px;
	font-size: 15px;
	width: 100px;
	height: 30px;
	line-height: 30px;
	color: #3CB371;
	border: 1px solid #3CB371;
	text-align: center;
	cursor: pointer;
	-webkit-transition: all 0.3s;
	border-radius: 3px;
}
.dialog-ok:hover{
	background: #3CB371;
	color: white;
}
.dialog-cancle{
	float: right;
	margin-right: 12px;
	font-size: 15px;
	width: 100px;
	height: 30px;
	line-height: 30px;
	color: #B22222;
	border: 1px solid #B22222;
	text-align: center;
	cursor: pointer;
	-webkit-transition: all 0.3s;
	border-radius: 3px;
}
.dialog-cancle:hover{
	background: #B22222;
	color: white;
}
.dialog-footer{
	height: 40px;
	padding-top: 8px;
	border-top: 1px solid #e1e1e1;
}
